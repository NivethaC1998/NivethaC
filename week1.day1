package week1.day1;

public class PrimeNumber {
	public static void main(String[] args) {
		int Number = 60;
		boolean flag = false;
		for (int i =2; i<Number/2; i++) {
			if  ((Number % i) == 0)
				flag = true;
		}
		if (flag)
			System.out.println("Not a Prime Number");
		else 
			System.out.println("Prime Number");
	}
}

